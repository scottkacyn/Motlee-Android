package com.motlee.android.object;

import com.j256.ormlite.field.DataType;
import com.j256.ormlite.field.DatabaseField;
import com.j256.ormlite.table.DatabaseTable;

@DatabaseTable(tableName = "temp_attendees")
public class TempAttendee {

	@DatabaseField(foreign = true, canBeNull = false, columnName = "event_detail")
	public EventDetail event_detail;
	
	@DatabaseField(columnName = "user_id", dataType = DataType.LONG_OBJ)
	public Long uid;
	
	@DatabaseField(columnName = "id", dataType = DataType.INTEGER_OBJ, generatedId = true)
	public Integer id;
	
	public TempAttendee()
	{
		
	}
	
	public TempAttendee(Long uid)
	{
		this.uid = uid;
	}
	
	public TempAttendee(Long uid, EventDetail eDetail)
	{
		this.uid = uid;
		this.event_detail = eDetail;
	}

    @Override
    public int hashCode() {
        int hashCode = 23;
        hashCode = hashCode + Integer.valueOf(uid.toString());
        hashCode = hashCode + id;
        return hashCode;
    }

    @Override
    public boolean equals(Object obj) {
        if (obj == null)
            return false;
        if (obj == this)
            return true;
        if (obj.getClass() != getClass())
            return false;

        TempAttendee that = (TempAttendee) obj;
        
        return
			( this.uid.equals(that.uid));
    }
	
	public int compareTo(TempAttendee another) {
		
		return this.uid.compareTo(another.uid);
	}
	
}
